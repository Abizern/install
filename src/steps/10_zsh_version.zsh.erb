# Check Zsh version
autoload -Uz is-at-least && if ! is-at-least <%= min_zsh_version %>; then
  print -u2 -P "%F{red}✗ You're using Zsh version ${ZSH_VERSION} and versions < <%= min_zsh_version %> are not supported. Please update your Zsh.%f"
  return 1
fi
print -P "%F{green}✓%f Using Zsh version ${ZSH_VERSION}"
